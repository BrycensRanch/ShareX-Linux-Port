<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:utils="clr-namespace:SnapX.Core.Utils;assembly=SnapX.Core"
             x:Class="SnapX.Avalonia.App"
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="avares://SnapX.Avalonia/Styling/Resources.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    <NativeMenu.Menu>
        <NativeMenu>
            <NativeMenuItem Header="{x:Static utils:Lang.AboutSnapX}" />
        </NativeMenu>
    </NativeMenu.Menu>
</Application>
